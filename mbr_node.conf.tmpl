[program:mbr_node]
command=/bin/bash _SITE_ROOT_/scripts/run loop _run _SITE_ROOT_
directory=_SITE_ROOT_
redirect_stderr=true
stdout_logfile=_SITE_ROOT_/logs/mbr_node.log
autorestart=true

[program:mbr_node_monitor]
command=/bin/bash _SITE_ROOT_/scripts/run loop _pull _SITE_ROOT_
directory=_SITE_ROOT_
redirect_stderr=true
stdout_logfile=_SITE_ROOT_/logs/mbr_node_monitor.log
autorestart=true
